{
  "cli": {
    "version": ">= 7.8.5"
  },
  "build": {
    "development-simulator": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true
      }
    },
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "ANDROID_SDK_ROOT": "/Users/samirgonzalez/Library/Android/sdk"
        }
    },
    "preview": {
      "android": {
        "env": {
          "ANDROID_SDK_ROOT": "/Users/samirgonzalez/Library/Android/sdk"
          },
